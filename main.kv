<Login_Screen>:
    name: 'Login_Screen'
    canvas.before:
        Color:
            rgba: 1, 1, 1, 1
        Rectangle:
            pos: self.pos
            size: self.size
    BoxLayout:
        id: box
        orientation: 'vertical'
        canvas.before:
        Button:
            id: Login_connector
            text: 'Fleet Retail Packaging'
            size_hint_y: None
            height: 40
            color: 1,1,1,1
            background_normal: ''
            background_color: 0, .4666, 0.70196, 1
        Widget:
            size_hint: 1, 0.05
        Image:
            source: 'login-user-png-7.png'
        Widget:
            size_hint: 1, 0.05
        BoxLayout:
            orientation: 'vertical'
            padding: 20,20,20,20
            spacing: 8
            Button:
                id: martin
                text: 'Martin Coles'
                background_normal: ''
                background_color: 0, .4666, 0.70196, 1
                on_press: root.setuserm()
                on_release: app.root.current = 'Selection_Screen'
            Button:
                text: 'Kieran Johnson'
                background_normal: ''
                background_color: 0, .4666, 0.70196, 1
                on_press: root.setuserk()
                on_release: app.root.current = 'Selection_Screen'
            Button:
                text: 'Joe Dewhurst'
                background_normal: ''
                background_color: 0, .4666, 0.70196, 1
                on_release: app.root.current = 'Selection_Screen'
                on_press: root.setuserj()
            Button:
                text: 'Peter Smith'
                background_normal: ''
                background_color: 0, .4666, 0.70196, 1
#                on_release: app.root.current = 'Selection_Screen'
                on_press: root.setuserp()
            Button:
                text: 'Reconnect'
                background_normal: ''
                background_color: 0, .4666, 0.70196, 1
                on_press: root.connect_to_server()



<Selection_Screen>:
